<table class="table table-hover table-sm table-responsive">
  <thead>
    <tr>
      <th>ID</th>
      <th>料理レシピ名</th>
      <th>登録日</th>
      <th>削除日</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @cuisines.each do |fc| %>
    <% if fc.discarded? %>
    <tr>
      <td><%= fc.id %></td>
      <td><%= fc.name %></td>
      <td><%= fc.discarded_at %></td>
      <td><%= link_to '復元', restore_managers_cuisine_path(fc.id),  class: "page-link text-dark d-inline-block", role: "button" %></td>
    </tr>
    <% end %>
    <% end %>
  </tbody>
</table>
